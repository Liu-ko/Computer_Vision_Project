function h = calculate_RGB_hist(pic)
    [rows, cols] = size(pic);
    h = zeros(1, 256, 3);
    for row = 1:rows
        for col = 1:cols
            red = pic(row, col, 1);
            green = pic(row, col, 2); 
            blue = pic(row, col, 3);
            h(red+1) = h(1, red+1, 1)+1;
            h(green+1) = h(1, green+1, 2)+1;
            h(1, blue+1,) = h(1, blue+1, 3)+1;
        end
    end
    for number = 1:256
        h(number) = h(number) / rows / cols;
    end
end